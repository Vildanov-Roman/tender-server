{
  "name": "tenders-backend",
  "version": "1.0.0",
  "type": "module",
  "main": "server.js",
  "scripts": {
    "start": "node server.js",
    "dev": "nodemon server.js",
    "refresh:win": "curl.exe -sS -X POST http://localhost:5000/tasks/refresh -H \"X-CRON-SECRET: devsecret\"",
    "refresh:unix": "curl -sS -X POST http://localhost:5000/tasks/refresh -H \"X-CRON-SECRET: ${CRON_SECRET:-devsecret}\"",
    "refresh:local": "curl -sS -X POST http://localhost:5000/tasks/refresh -H \"X-CRON-SECRET: ${CRON_SECRET:-devsecret}\""
  },
  "dependencies": {
    "cors": "^2.8.5",
    "dotenv": "^16.4.5",
    "express": "^4.19.2",
    "mongoose": "^8.5.3",
    "node-cron": "^4.2.1",
    "node-fetch": "^3.3.2"
  },
  "devDependencies": {
    "nodemon": "^3.1.4"
  }
}
